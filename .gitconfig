[user]
	name = Geoff Catlin
	email = gcatlin@gmail.com
[alias]
	co = checkout
	switch = !legit switch \"$@\"
	branches = !legit branches
	sprout = !legit sprout \"$@\"
	unpublish = !legit unpublish \"$@\"
	harvest = !legit harvest \"$@\"
	sync = !legit sync \"$@\"
	publish = !legit publish \"$@\"
	graft = !legit graft \"$@\"
[apply]
	whitespace = nowarn
[color]
	branch = auto
	diff = auto
	status = auto
	ui = auto
[core]
	autocrlf = input
	excludesfile = ~/.gitignore_global
	pager = less
[format]
	pretty = %C(yellow)%h%Creset %s %C(red)(%an, %cr)%Creset
[github]
	user = gcatlin
	token = c49f467170498ecf398ef977cf9b0951
[push]
	default = simple
[diff]
	algorithm = patience
[filter "media"]
	clean = git-media-clean %f
	smudge = git-media-smudge %f
